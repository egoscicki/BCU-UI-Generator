# BCU Design Studio Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_MODEL=gpt-4
REACT_APP_OPENAI_MAX_TOKENS=2000

# App Configuration
REACT_APP_APP_NAME=BCU Design Studio
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_ENVIRONMENT=development

# Optional: Analytics and Monitoring
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Optional: Feature Flags
REACT_APP_ENABLE_AI_GENERATION=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_COLLABORATION=false
